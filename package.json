{
  "name": "fabric-vizpath",
  "version": "1.0.0",
  "description": "A path editor for Fabric.js(http://fabricjs.com)",
  "homepage": "https://huanjinliu.github.io/fabric-vizpath",
  "main": "dist/index.min.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "pnpm link . && npm run dev",
    "dev": "rollup --config --watch --environment NODE_ENV:development,ROLLUP_WATCH:true",
    "build:docs": "rollup --config --environment NODE_ENV:development",
    "build": "rollup --config --environment NODE_ENV:production"
  },
  "author": "Hain <huanjin.liu@foxmail.com>",
  "files": [
    "dist",
    "global.d.ts",
    "docs",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/huanjinliu/fabric-vizpath.git"
  },
  "bugs": {
    "url": "https://github.com/huanjinliu/fabric-vizpath/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ],
    "./*.{js,json} !./package-lock.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.7",
    "@eslint/js": "^9.4.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bezier-js": "^4.1.3",
    "@types/fabric": "^5.3.7",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.1",
    "@types/react": "^18.3.3",
    "classnames": "^2.5.1",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fabric": "^5.3.0",
    "github-markdown-css": "^5.6.1",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.5",
    "markdown-it": "^14.1.0",
    "postcss": "^8.4.39",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.9.6",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.11.0",
    "vivid-wait": "^1.1.1"
  },
  "peerDependencies": {
    "fabric": "^5.3.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "bezier-js": "^6.1.4"
  }
}
